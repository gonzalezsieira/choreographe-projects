                            <script language="4">
                                <content>
                                    <![CDATA[from time import sleep
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        pass

    def onLoad(self):
        self.memory = ALProxy("ALMemory")
        self.memory.insertData("FrontTactilTouched", '0')
        self.memory.insertData("RearTactilTouched", '0')
        self.memory.insertData("MiddleTactilTouched", '0')
    def onUnload(self):
        self.memory = None

    def onInput_FrontTactilTouched(self, p):
            self.memory.raiseEvent("FrontTactilTouched", '1')
            sleep(0.1)
            self.memory.insertData("FrontTactilTouched", '0')
    def onInput_MiddleTactilTouched(self, p):
            self.memory.raiseEvent("MiddleTactilTouched", '1')
            sleep(0.1)
            self.memory.insertData("MiddleTactilTouched", '0')
    def onInput_RearTactilTouched(self, p):
            self.memory.raiseEvent("RearTactilTouched", '1')
            sleep(0.1)
            self.memory.insertData("RearTactilTouched", '0')]]>
</content>
